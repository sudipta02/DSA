https://leetcode.com/problems/two-sum/description/

    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int, int> mp;
        int n = nums.size();
        for(int i = 0; i < n; i++){
            int ele2 = target - nums[i];
            if(mp.find(ele2) != mp.end()){
                return {i, mp[ele2]};
            }
            mp[nums[i]] = i;
        }
        return {-1, -1};
    }